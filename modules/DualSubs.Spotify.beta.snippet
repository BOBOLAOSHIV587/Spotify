#!name=🍿️ DualSubs: 🎵 Spotify β
#!desc=(BETA) Spotify歌词增强及双语模块
#!openUrl=http://boxjs.com/#/app/DualSubs.Spotify.beta
#!author=VirgilClyne
#!homepage=https://github.com/DualSubs
#!manual=https://github.com/DualSubs/Spotify/wiki/🍿-DualSubs:-🎵-Spotify
#!icon=https://raw.githubusercontent.com/Koolson/Qure/master/IconSet/Color/Spotify.png

#[rewrite_local]
# DualSubs.Spotify.Tracks.response
^https?:\/\/api\.spotify\.com\/v1\/tracks\? url script-response-body https://raw.githubusercontent.com/DualSubs/Spotify/beta/js/DualSubs.Spotify.response.beta.js

# DualSubs.Spotify.Lyrics.request
^https?:\/\/spclient\.wg\.spotify\.com\/color-lyrics\/v2\/track\/(.+)\?(.*) url script-request-body https://raw.githubusercontent.com/DualSubs/Spotify/beta/js/DualSubs.Spotify.request.beta.js

# DualSubs.Spotify.Lyrics.Translate.response
^https?:\/\/spclient\.wg\.spotify\.com\/color-lyrics\/v2\/track\/(.+)\?(.*)subtype=Translate url script-response-body https://raw.githubusercontent.com/DualSubs/Universal/beta/js/DualSubs.Subtitles.Translate.response.beta.js

# DualSubs.Spotify.Lyrics.External.response
^https?:\/\/spclient\.wg\.spotify\.com\/color-lyrics\/v2\/track\/(.+)\?(.*)subtype=External url script-response-body https://raw.githubusercontent.com/DualSubs/Universal/beta/js/DualSubs.Lyrics.External.response.beta.js

#[mitm]
hostname = api.spotify.com, spclient.wg.spotify.com
