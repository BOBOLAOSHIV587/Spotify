{
	"name": "@dualsubs/youtube",
	"version": "1.9.0",
	"organizationName": "🍿️ DualSubs",
	"displayName": "🍿️ DualSubs: 🎵 Spotify",
	"description": "Spotify 增强及双语歌词",
	"homepage": "https://DualSubs.github.io/guide/spotify",
	"openUrl": "http://boxjs.com/#/app/DualSubs.Spotify",
	"icon": "https://github.com/DualSubs/Spotify/raw/main/src/assets/icon_rounded.png",
	"keywords": [],
	"contributors": [
		"VirgilClyne[https://github.com/VirgilClyne]"
	],
	"system": [
		"iOS",
		"iPadOS",
		"macOS"
	],
	"systemVersion": 15,
	"license": "Apache-2.0",
	"type": "module",
	"repository": {
		"type": "git",
		"url": "https://github.com/DualSubs/Spotify.git"
	},
	"directories": {
		"example": "example"
	},
	"scripts": {
		"serve": "webpack serve",
		"build": "rspack build",
		"build:dev": "rspack build -c rspack.dev.config.js --mode=development",
		"build:args": "arguments-builder build",
		"dts": "arguments-builder dts",
		"test": "rollup --config --configDebug",
		"Lyrics": "protoc --ts_out ./src/protobuf --ts_opt generate_dependencies --ts_opt long_type_number --ts_opt output_javascript --proto_path ./src/protobuf Lyrics.proto",
		"Bootstrap": "protoc --ts_out ./src/protobuf --ts_opt generate_dependencies --ts_opt long_type_number --ts_opt output_javascript --proto_path ./src/protobuf bootstrap.new.proto",
		"ExtendedMetadata": "protoc --ts_out ./src/protobuf --ts_opt generate_dependencies --ts_opt long_type_number --ts_opt output_javascript --proto_path ./src/protobuf ExtendedMetadata.proto"
	},
	"browserslist": [
		"iOS >= 15"
	],
	"devDependencies": {
		"@iringo/arguments-builder": "^1.7.7",
		"@iringo/sgmoudle-tools": "^1.2.2",
		"@protobuf-ts/plugin": "^2.9.3",
		"@protobuf-ts/runtime": "^2.9.3",
		"@rspack/cli": "^1.0.13",
		"@rspack/core": "^1.0.13"
	},
	"dependencies": {
		"@protobuf-ts/runtime": "^2.9.3",
		"@protobuf-ts/runtime-rpc": "^2.9.3",
		"node-polyfill-webpack-plugin": "^4.0.0"
	}
}
